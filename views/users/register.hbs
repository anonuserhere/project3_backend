{{#extends "base"}}
  {{#block "title"}}Register New Account{{/block}}
  {{#block "body"}}
    <h2 class="ms-2 mt-1">Register</h2>
    <div>
      <form method="post" class="m-4" id="form">
        {{{form}}}
        <input type="hidden" name="_csrf" value="{{csrfToken}}" />
        <div class="mt-2">
          <input type="submit" value="Register" class="btn btn-primary" />
        </div>
      </form>
    </div>
  {{/block}}
{{/extends}}